<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ReserveDetail-Mapper">
	<select id="selectReserveDetail" parameterType="int" resultType="reserveDetail">
	select *
	from RESERVE_LIST
	where rsno = #{rsno}
	</select>
</mapper>