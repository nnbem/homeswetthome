<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdoptionDetail-Mapper">
	<select id="selectAdoptionDetail" parameterType="int" resultType="AdoptionDetail">
	select *
    from ADOPTION_LIST
    where ano = #{ano}
	</select>
</mapper>